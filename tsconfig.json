{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": false,
    "baseUrl": "src/app",
    "paths": {
      "#/*": [
        "*"
      ]
    }
  },
  "include": [
    "src/app/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}